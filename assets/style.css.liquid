    background-size: cover;
    background-repeat: no-repeat;
    background-position: bottom left;
}

a.menu-btn span {
    display: block;
    position: absolute;
    height: 3px;
    width: 48px;
    top: 15px;
    background-color: #fff;
    left: 35px;
    transition: all 0.5s;
    border-radius: 3px;
}

a.menu-btn span:nth-child(2) {
    top: 27px;
}

a.menu-btn span:nth-child(3) {
    top: 39px;
}

a.menu-btn::after {
    content: 'MENU';
    left: 32px;
    top: 45px;
    position: absolute;
    color: #fff;
    width: fit-content;
    text-align: center;
    line-height: 1.72;
    font-family: "Shippori Mincho",serif;
    font-size: 16px;
    font-weight: 500;
    transition: all .3s;
}

a.menu-btn:hover {
    opacity: 1;
}

a.menu-btn.open:after {
    content: "CLOSE";
    display: block;
}
a.menu-btn.open span {
    display: block;
    position: absolute;
    transform: rotate(45deg);
    top: 23px;
    left: 35px;
}

a.menu-btn.open span:nth-child(2) {
    display: none;
}

a.menu-btn.open span:nth-child(3) {
    transform: rotate(-45deg);
}
a.menu-btn.open {
    position: fixed;
}

